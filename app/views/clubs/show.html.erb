<% @club.images.each do |image| %>
  <div class="logo">
    <%= image_tag image %>
  </div>
<% end %>
<div id="club name">
  <h1><%= @club.name %></h1>
</div>
<div id="mission statement">
  <p><%= @club.description %></p>
</div>
<div id="edit button">
  <% if current_user && current_user.acc_type == "club officer" %>
    <%= button_to "Edit",  edit_club_path(@club), :method => :get  %>
  <% end %>

</div>
